	<script>
		// Fonction pour mettre à jour l'aperçu dans le modal
		function updateModalPreview() {
			const text = document.querySelector('input[name="product[text]"]')?.value || '';
			const largeur = document.querySelector('input[name="product[largeur]"]')?.value || '';
			const hauteur = document.querySelector('input[name="product[hauteur]"]')?.value || '';
			const typeEcriture = document.querySelector('select[name="product[typeEcriture]"]')?.value || '';
			
			document.getElementById('modal-preview-text').textContent = text || '--';
			document.getElementById('modal-preview-width').textContent = largeur ? largeur : '--';
			document.getElementById('modal-preview-height').textContent = hauteur ? hauteur : '--';
			document.getElementById('modal-preview-font').textContent = typeEcriture || '--';
		}

		$(document).ready(function() {
			// Ouvrir le modal quand on clique sur Quick View
			$('.js-show-modal1').on('click', function(e) {
				e.preventDefault();
				$('.js-modal1').fadeIn();
			});
			
			// Fermer le modal avec le bouton X
			$('.js-hide-modal1').on('click', function() {
				$('.js-modal1').fadeOut();
			});
			
			// Fermer le modal en cliquant sur l'overlay
			$('.overlay-modal1').on('click', function() {
				$('.js-modal1').fadeOut();
			});

			// Gérer la soumission du formulaire
			$('#product-form').on('submit', function(e) {
				e.preventDefault();
				
				// Récupérer les données du formulaire
				const formData = new FormData(this);
				
				// Envoyer les données via AJAX
				$.ajax({
					url: $(this).attr('action'),
					method: 'POST',
					data: formData,
					processData: false,
					contentType: false,
					success: function(response) {
						if (response.success && response.productId) {
							// Ajouter automatiquement au panier
							fetch(`/cart/add/${response.productId}`, {
								method: 'POST',
								headers: {
									'Content-Type': 'application/x-www-form-urlencoded',
								},
								body: 'quantity=1&price=10.00'
							})
							.then(cartResponse => cartResponse.json())
							.then(cartData => {
								// Mettre à jour le compteur du panier
								updateCartCounter(cartData.cartItemsCount);
								
								// Fermer le modal
								$('.js-modal1').fadeOut();
								
								// Afficher un message de succès
								swal("Produit ajouté!", "Votre produit a été créé et ajouté au panier!", "success");
								
								// Réinitialiser le formulaire
								$('#product-form')[0].reset();
								updateModalPreview();
							});
						} else {
							$('.js-modal1').fadeOut();
							swal("Produit créé!", "Votre produit a été créé avec succès!", "success");
							$('#product-form')[0].reset();
							updateModalPreview();
						}
					},
					error: function() {
						swal("Erreur!", "Une erreur s'est produite lors de la création du produit.", "error");
					}
				});
			});

			// Fonction pour mettre à jour le compteur du panier
			window.updateCartCounter = function(count) {
				$('.js-show-cart').attr('data-notify', count);
				if (count > 0) {
					$('.js-show-cart').addClass('icon-header-noti');
				} else {
					$('.js-show-cart').removeClass('icon-header-noti');
				}
			};

			// Charger le compteur du panier au chargement de la page
			fetch('/cart/count')
				.then(response => response.json())
				.then(data => {
					updateCartCounter(data.count);
				})
				.catch(error => console.log('Erreur chargement panier:', error));
		});
	</script>

	<!-- Cart Functions -->
	<script src="js/cart-functions.js"></script>

	<!-- Modal Product Quick View -->
	{% include 'includes/product_modal.html.twig

</body>
